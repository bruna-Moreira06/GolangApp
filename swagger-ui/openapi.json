{
	"components": {
		"schemas": {
			"CatId": {
				"format": "uuid",
				"type": "string"
			},
			"CatProto": {
				"properties": {
					"birthDate": {
						"example": "2023-02-14",
						"type": "string"
					},
					"color": {
						"example": "blue",
						"type": "string"
					},
					"name": {
						"example": "Felix",
						"type": "string"
					}
				},
				"type": "object"
			}
		}
	},
	"info": {
		"title": "Cats demo REST API used to manage a local database of üêà",
		"version": "1.0.0"
	},
	"openapi": "3.0.1",
	"paths": {
		"/cats": {
			"get": {
				"responses": {
					"200": {
						"description": "Success"
					}
				},
				"summary": "Lists all cats",
				"tags": [
					"cats"
				]
			},
			"post": {
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/CatProto"
							}
						}
					},
					"description": "The proto cat",
					"required": true
				},
				"responses": {
					"201": {
						"description": "Created"
					}
				},
				"summary": "Creates a new cat",
				"tags": [
					"cats"
				]
			}
		},
		"/cats/{catId}": {
			"delete": {
				"parameters": [
					{
						"in": "path",
						"name": "catId",
						"required": true,
						"schema": {
							"$ref": "#/components/schemas/CatId"
						}
					}
				],
				"responses": {
					"204": {
						"description": "The ref was deleted"
					},
					"404": {
						"description": "Not found"
					}
				},
				"summary": "Deletes a cat",
				"tags": [
					"cats"
				]
			},
			"get": {
				"parameters": [
					{
						"in": "path",
						"name": "catId",
						"required": true,
						"schema": {
							"$ref": "#/components/schemas/CatId"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Success"
					},
					"404": {
						"description": "Not found"
					}
				},
				"summary": "Gets a cat details",
				"tags": [
					"cats"
				]
			}
		}
	},
	"servers": [
		{
			"url": "../api"
		}
	]
}
